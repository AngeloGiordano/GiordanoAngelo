					<modifiers optional="false" unique="true" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="position">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="Cyclist" qualifier="name">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="time">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
