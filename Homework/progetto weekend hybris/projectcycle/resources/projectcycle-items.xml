<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	
	<enumtypes>
		<enumtype code="stageParticipation" autocreate="true" generate="true"
			dynamic="true">
			<value code="participant" />
			<value code="retired" />
		</enumtype>
	</enumtypes>
		<relations>
		<!-- relazione 1-n tra StageRace e Stage -->
		<relation localized="false" code="StageRace2Stage">
			<sourceElement type="StageRace" qualifier="stagerace"
				cardinality="one">
				<modifiers optional="true" />
			</sourceElement>
			<targetElement type="Stage" qualifier="stages"
				cardinality="many">
				<modifiers optional="true" />
			</targetElement>
		</relation>
		
		<!-- relazione 1-n tra StageRanking e Stage -->
		
		<relation localized="false" code="StageRanking2Stage">
			<sourceElement type="StageRanking" qualifier="stageranking"
				cardinality="one">
				<modifiers optional="true" />
			</sourceElement>
			<targetElement type="Stage" qualifier="stages"
				cardinality="many">
				<modifiers optional="true" />
			</targetElement>
		</relation>
		
		<!-- relazione n-n tra Stage e Cyclist -->
		
		<relation code="Stage2CyclistRelation" generate="true" autocreate="true"
		localized="false">
		<deployment table="Stage2CyclistRel" typecode="11000"/>
		<sourceElement qualifier="stages" type="Stage" cardinality="many"/>
		<targetElement qualifier="cyclists" type="Cyclist"
		 cardinality="many"/>
		</relation>
		</relations>
		
		<itemtypes>
		<!-- item type della tabella StageRace -->
		<itemtype code="StageRace" autocreate="true" generate="true">
			<deployment table="StageRace" typecode="15554" />
			<attributes>
				<attribute qualifier="name" type="java.lang.String">
					<modifiers optional="false" unique="true" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="numberstage" type="java.lang.Integer">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="Date" type="java.util.Date">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
		</itemtype>

		
		
		<!-- item type della tabella Stage -->
		<itemtype code="Stage" autocreate="true" generate="true">
			<deployment table="Subject" typecode="15557" />
			<attributes>
				<attribute type="java.lang.String" qualifier="uid">
					<modifiers optional="false" unique="true" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="localized:java.lang.String" qualifier="name">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="distance">
					<modifiers optional="true" unique="false" />
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>
		
		
		<!-- item type della tabella Cyclist -->
		<itemtype code="Cyclist" autocreate="true" generate="true">
			<deployment table="Cyclist" typecode="15558" />
			<attributes>
				<attribute type="java.lang.String" qualifier="uid">
					<modifiers optional="false" unique="true" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="name">
					<modifiers optional="true" unique="false" />
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="team">
					<modifiers optional="true" unique="false" />
